{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_CONNECTION_STRING}"
  },
  "OrchardCore": {
    "Database": {
      "DataProvider": "SqlConnection",
      "DataConnectionString": "DefaultConnection"
    },
    "OrchardCore_Setup": {
      "DefaultCulture": "hr",
      "SupportedCultures": [ "hr", "en" ]
    }
  },
  "AllowedHosts": "*"
}
