
<main class="page-wrapper ">

        <div class="container">
            {% assign imagePath = Model.ContentItem.Content.Blog.Image.Paths.first %}

            <div class="row d-flex flex-column justify-content-end content-center" style="height: 10vh; background-image: url('{{ imagePath | asset_url | resize_url: profile: 'profile800x600', anchor: anchor }}')">
                <!-- Page Header -->
                <!-- Set your background image for this header on the line below. -->

                    

            </div>
        </div>
        
       
        {% shape_remove_item Model.Content "HtmlBodyPart" %}
        {% shape_remove_item Model.Content "Blog-Image" %}
        {% shape_remove_item Model.Content "ContentsMetadata" %}
        {{Model.Content | shape_render}}
        
</main>

